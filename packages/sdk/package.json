{
  "name": "@llmonitor/sdk",
  "version": "0.1.10",
  "description": "Universal LLM observability & cost intelligence - Monitor OpenAI, Anthropic, Google AI, Cohere and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm version patch && standard-version",
    "postinstall": "node postinstall.js"
  },
  "keywords": [
    "llm",
    "monitoring",
    "observability",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "gemini",
    "cohere",
    "cost-tracking",
    "analytics",
    "express",
    "middleware",
    "ai",
    "machine-learning"
  ],
  "author": "LLMonitor Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agusgarcia3007/LLMonitor",
    "directory": "packages/sdk"
  },
  "homepage": "https://llmonitor.io",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@types/node": "^20.0.0",
    "husky": "^9.1.7",
    "standard-version": "^9.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.20.0",
    "@google/generative-ai": ">=0.11.0",
    "cohere-ai": ">=7.0.0",
    "express": ">=4.0.0",
    "openai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    },
    "cohere-ai": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  }
}
